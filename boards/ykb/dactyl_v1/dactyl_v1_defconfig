# Right would be +1
CONFIG_YKB_USBD_PID_LEFT=0x0001
# 125*2ma=250ma MAX
CONFIG_YKB_USBD_MAX_POWER=125
CONFIG_YKB_USBD_REMOTE_WAKEUP=y
